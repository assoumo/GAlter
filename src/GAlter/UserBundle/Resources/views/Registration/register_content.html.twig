{% trans_default_domain 'FOSUserBundle' %}

<form action="
    {% if app.request.attributes.get('_route') == 'user_one_registration' %}
    {{ path('user_one_registration') }}
    {% endif %}

    {% if app.request.attributes.get('_route') == 'user_two_registration' %}
    {{ path('user_two_registration') }}
    {% endif %}

     {% if app.request.attributes.get('_route') == 'user_three_registration' %}
    {{ path('user_three_registration') }}
    {% endif %}


    {% if app.request.attributes.get('_route') == 'user_four_registration' %}
    {{ path('user_four_registration') }}
    {% endif %}"  {{ form_enctype(form) }} method="POST"  class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" />
    </div>
</form>