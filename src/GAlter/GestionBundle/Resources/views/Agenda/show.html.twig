{%  extends "GAlterUserBundle::layout.html.twig" %}
{% block corps%}
    <h1>Agenda</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Lieu</th>
                <td>{{ entity.lieu }}</td>
            </tr>
            <tr>
                <th>Heure</th>
                <td>{{ entity.heure|date('H:i:s') }}</td>
            </tr>
        </tbody>
    </table>


{% endblock %}
